{
    "name": "createsudokupuzzle",
    "version": "0.0.1",
    "description": "Generate and solve sudoku puzzles easily!",
    "keywords": [
        "sudoku",
        "sudoku-generator",
        "sudoku-solver",
        "sudoku-puzzle",
        "sudokupuzzle",
        "createSudoku",
        "create-sudoku",
        "sudoku-creator"
    ],
    "homepage": "https://github.com/dave9123/createSudoku#readme",
    "bugs": {
        "url": "https://github.com/dave9123/createSudoku/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dave9123/createSudoku.git"
    },
    "license": "MIT",
    "author": "dave9123",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "type": "module",
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "package.json",
        "package-lock.json"
    ],
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --minify --out-dir dist",
        "test": "jest",
        "prepare": "npm run build",
        "publish": "npm run prepare && npm publish --access public"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "tsup": "^8.3.5",
        "typescript": "^5.7.3"
    }
}