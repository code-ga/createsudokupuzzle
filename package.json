{
    "name": "sudokupuzzle",
    "version": "0.0.0-dev",
    "description": "Generate and solve sudoku puzzles",
    "keywords": [
        "sudoku",
        "sudoku-generator",
        "sudoku-solver",
        "sudoku-puzzle",
        "sudokupuzzle"
    ],
    "homepage": "https://github.com/dave9123/sudokuPuzzle#readme",
    "bugs": {
        "url": "https://github.com/dave9123/sudokuPuzzle/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dave9123/sudokuPuzzle.git"
    },
    "license": "GPL-3.0-or-later",
    "author": "dave9123",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "type": "module",
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --minify --out-dir dist",
        "test": "jest",
        "prepare": "npm run build",
        "publish": "npm run prepare && npm publish"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "tsup": "^8.3.5",
        "typescript": "^5.7.3"
    },
    "dependencies": {
        "@babel/preset-typescript": "^7.26.0"
    }
}
